{
  "name": "storeon-observable",
  "version": "0.2.0",
  "description": "RxJS based module for Storeon which allows to create async actions",
  "main": "index.js",
  "sideEffects": false,
  "repository": "git@github.com:distolma/storeon-observable.git",
  "author": "Dmytro Mostovyi <djmakaron2009@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development parcel example/index.html --open",
    "lint": "eslint *.js",
    "test": "jest --coverage && yarn lint && size-limit"
  },
  "peerDependencies": {
    "rxjs": ">=6.0.0-beta.0 <7"
  },
  "devDependencies": {
    "@logux/eslint-config": "^36.0.0",
    "@size-limit/preset-small-lib": "^4.0.2",
    "babel-preset-react-app": "^9.1.1",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-es5": "^1.5.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prefer-let": "^1.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^16.1.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "parcel-bundler": "^1.12.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "size-limit": "^4.0.2",
    "storeon": "^0.9.8",
    "typescript": "^3.8.2"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "size-limit": [
    {
      "path": "index.js",
      "limit": "226 B"
    }
  ],
  "eslintConfig": {
    "extends": "@logux/eslint-config",
    "rules": {
      "node/no-extraneous-require": "off",
      "func-style": "off"
    }
  }
}
